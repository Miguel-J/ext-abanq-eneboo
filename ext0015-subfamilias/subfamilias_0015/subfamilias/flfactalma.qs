//// SUBFAMILIAS ////////////////////////////////////////////
class subfamilias extends oficial
{
  function subfamilias(context)
  {
    oficial(context);
  }
  function beforeCommit_subfamilias(curSF)
  {
    return this.ctx.subfamilias_beforeCommit_subfamilias(curSF);
  }
}
//// SUBFAMILIAS ////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

/** @class_definition subfamilias */
/////////////////////////////////////////////////////////////////
//// SUBFAMILIAS ////////////////////////////////////////////
function subfamilias_beforeCommit_subfamilias(curSF)
{
  var _i = this.iface;

  if (!_i.controlDatosMod(curSF)) {
    return false;
  }

  return true;
}
//// SUBFAMILIAS ////////////////////////////////////////////
